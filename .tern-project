{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**/*.js",
    "build/**/*.js",
    "dist/**/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    }
  }
}
